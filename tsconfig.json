{
	"compilerOptions": {
		"rootDir": "src",
		"outDir": "dist",
		"target": "ESNext",
		"module": "NodeNext",
		"moduleResolution": "NodeNext",
		"strict": true,
		"skipLibCheck": true,
		"esModuleInterop": true,
		"verbatimModuleSyntax": true,
		"types": ["node"],
		"jsx": "react-jsx",
		"jsxImportSource": "react",
		"sourceMap": true
	},
	"exclude": ["node_modules"]
}
